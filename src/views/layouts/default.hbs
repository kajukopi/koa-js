<!DOCTYPE html>
<html lang="en">

    <head>
        {{> meta }}
    </head>

    <body style="background-color: #e8eaf6">
        {{> bg }}

        {{> header}}

        <main id="content" style="margin-top: 58px">
            <section class="container my-5 py-2">
                <div class="card mb-2 pb-0 pt-0">
                    <div class="card-body d-flex pb-0 pt-3">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="#">Home</a></li>
                                <li class="breadcrumb-item active" aria-current="page">{{ title }}</li>
                            </ol>
                        </nav>
                        <div class="text-muted ms-auto" id="dateTime"></div>
                    </div>
                </div>
                {{{content}}}
            </section>
        </main>

        {{> footer}}
    </body>


</html>